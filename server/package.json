{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "console": "babel-node src/index.js console",
    "start": "npm run dev:start",
    "dev:start": "nodemon --ignore build --exec babel-node src",
    "prod:build": "rimraf build && babel src -d build --ignore __test__,.test.js",
    "prod:start": "cross-env NODE_ENV=production pm2 start build --name tblog-server && pm2 logs",
    "prod:stop": "pm2 delete tblog-server",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "create:migration": "sequelize migration:create"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.6",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "errorhandler": "^1.5.0",
    "fs": "^0.0.1-security",
    "kcors": "^2.2.1",
    "koa": "^2.2.0",
    "koa-body": "^2.1.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.4",
    "koa-json": "^2.0.2",
    "koa-methodoverride": "^2.0.0",
    "koa-router": "^7.2.0",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "sequelize": "^4.0.0",
    "sequelize-cli": "^2.5.1"
  },
  "license": "MIT"
}
