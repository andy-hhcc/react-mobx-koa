{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "nodemon --ignore build --exec babel-node src",
    "prod:build": "rimraf build && babel src -d build --ignore __test__,.test.js",
    "prod:start": "cross-env NODE_ENV=production pm2 start build --name tblog-server && pm2 logs",
    "prod:stop": "pm2 delete tblog-server"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.6",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "errorhandler": "^1.5.0",
    "fs": "^0.0.1-security",
    "koa": "^2.2.0",
    "koa-router": "^7.2.0",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "path": "^0.12.7"
  },
  "license": "MIT"
}
